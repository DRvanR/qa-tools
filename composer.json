{
    "name": "ibuildings/qa-php",
    "description": "Ibuildings standard QA for PHP",
    "require": {
        "php": ">=5.3.3",
        "squizlabs/php_codesniffer": "1.4.4",
        "phpmd/phpmd": "1.4.1",
        "pdepend/pdepend": "1.1.0",
        "symfony/console": "v2.2.0",
        "twig/twig": "v1.12.2"
    },
    "bin": [
        "bin/phpdocumentor",
        "bin/phpunit",
        "bin/scrutinizer",
        "bin/php-cs-fixer",
        "bin/console"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload" : {
        "psr-0": {
            "Ibuildings": "src/"
        }
    }
}
